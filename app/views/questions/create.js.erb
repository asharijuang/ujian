question = $('.questions');
question.append();
answers.append("<p><%= question.question %></p>
    <%= link_to 'Destroy', exam_question, method: :delete, data: { confirm: 'Are you sure?' } %>

    <div class="answers">
      <% question.answers.each do |q| %>
        <p> <%= q.name %> </p>
      <% end %>
    </div>

    <%= simple_form_for([question, @answer], remote: true) do |f| %>
	  <%= f.error_notification %>

	  <div class="form-inputs">
      answer
	    <%= f.input :name %>
	  </div>

	  <div class="form-actions">
	    <%= f.button :submit %>
	  </div>
	<% end %>
  ------------------------------------------------");